<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="OverviewTaskFlow">
    <default-activity>OverviewView</default-activity>
    <view id="OverviewView">
      <page>/nl.whitehorses.tornado.Overview/OverviewView.amx</page>
    </view>
    <task-flow-call id="SettingsTaskFlow">
      <task-flow-reference>
        <document>/nl.whitehorses.tornado.Settings/SettingsTaskFlow.xml</document>
        <id>SettingsTaskFlow</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>OverviewView</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>toSettings</from-outcome>
        <to-activity-id>SettingsTaskFlow</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
