<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="SettingsTaskFlow">
    <default-activity>SettingsView</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>settingsBean</managed-bean-name>
      <managed-bean-class>nl.whitehorses.tornado.beans.SettingsBean</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="SettingsView">
      <page>/nl.whitehorses.tornado.Settings/SettingsView.amx</page>
    </view>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
